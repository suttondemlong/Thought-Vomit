l>\n            <textarea\n              className=\"text-area\"\n              name='content'\n              cols='40'\n              rows='20'\n              value={formData.content}\n              onChange={handleChange}\n            />\n          </label>\n        </div>\n        <div className='checkboxes'>  \n          {\n          moods.map((mood) => (\n            <label className=\"checkbox-label\">\n              <input\n                className=\"checkbox-input\"\n                type='checkbox'\n                checked={chosenMoods.includes(mood.id)}\n                value={mood.id}\n                onChange={handleCheckBoxChange}\n              />\n              {mood.name}\n            </label>\n            ))\n          }   \n        </div>\n      </div>\n        <button className=\"button\" id=\"keep-button\">save</button>\n        <button\n          className=\"button\"\n          id=\"trash-button\"\n          onClick={(e) => {\n          e.preventDefault();\n          props.handleDelete(id);\n          history.push('/thoughts');\n        }}>trash</button>\n    </form>\n    </div>\n  );\n}\n\nexport default ThoughtEdit;",["76","77"],"/Users/suttondemlong/General_Assembly/sei/unit_4/project-4/Thought-Vomit/client/src/index.js",[],"/Users/suttondemlong/General_Assembly/sei/unit_4/project-4/Thought-Vomit/client/src/screens/Register.jsx",[],"/Users/suttondemlong/General_Assembly/sei/unit_4/project-4/Thought-Vomit/client/src/services/apiConfig.js",[],"/Users/suttondemlong/General_Assembly/sei/unit_4/project-4/Thought-Vomit/client/src/components/Moods.jsx",[],"/Users/suttondemlong/General_Assembly/sei/unit_4/project-4/Thought-Vomit/client/src/services/moods.js",[],"/Users/suttondemlong/General_Assembly/sei/unit_4/project-4/Thought-Vomit/client/src/screens/ThoughtDetail.jsx",[],"/Users/suttondemlong/General_Assembly/sei/unit_4/project-4/Thought-Vomit/client/src/screens/About.jsx",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ru